<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
    <%- include('partials/head.ejs') %>

<body>
	<!-- Header -->
	    <%- include('partials/headerLogueado.ejs') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
            
			<br>
                <form action="/product/productadd" method="get">
					<div>
						<label for="file">Cargar Archivo:</label>
						<input type="file" name="image" value="">
					</div>
					<div>
						<label for="text">Nombre del producto:</label>
						<input type="text" name="product" value="">
					</div>
					<div>
						<label for="text">Descripción:</label>
						<input type="text" name="description" value="">
					</div>
					<div>
						<label for="date">Fecha de publicación:</label>
						<input type="date" name="date" value="">
					</div>
                    <div>
                        <button type="submit">AGREGAR PRODUCTO</button>
                    </div>
				</form>
				<br>
				<br>
			


	<!-- footer -->
        <%- include('partials/footer.ejs') %>
	<!-- /footer -->
</body>

</html>